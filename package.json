{
  "name": "gatsby-starter-styled",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and styled-system",
  "version": "2.0.0",
  "author": "Gregor Albrecht <hello@gregoralbrecht.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled"
  },
  "bugs": {
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled/issues"
  },
  "keywords": [
    "react",
    "gatsby",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "gatsby develop -o",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{jsx,js}\"",
    "lint:scripts": "eslint './src/**/*.{jsx,js}'",
    "lint:styles": "stylelint './src/**/*.{jsx,js}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint:styles && npm run lint:scripts"
    }
  },
  "dependencies": {
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.19.32",
    "gatsby-background-image": "^0.10.2",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-catch-links": "^2.1.27",
    "gatsby-plugin-google-analytics": "^2.1.37",
    "gatsby-plugin-lodash": "^3.1.21",
    "gatsby-plugin-manifest": "^2.2.44",
    "gatsby-plugin-offline": "^3.0.37",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-scroll-reveal": "0.0.6",
    "gatsby-plugin-sharp": "^2.4.7",
    "gatsby-plugin-sitemap": "^2.2.28",
    "gatsby-plugin-styled-components": "^3.1.20",
    "gatsby-plugin-typography": "^2.3.23",
    "gatsby-source-filesystem": "^2.1.50",
    "gatsby-transformer-sharp": "^2.3.17",
    "intersection-observer": "^0.7.0",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-typist": "^2.0.5",
    "react-typist-loop": "0.0.5",
    "react-typography": "^0.16.19",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.31",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
